mixin filaAlumno(alumno)
	-var fechaNueva = moment(alumno.fechanacimiento).format('dd/MM/yyyy')
	tr
		td= alumno.nombre
		td= alumno.matricular 
		td= alumno.email
		td= fechaNueva
		


